<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kanfs.omas.mapper.RecordMapper">

    <resultMap id="BaseResultMap" type="com.kanfs.omas.pojo.Record">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="userId" column="userId" jdbcType="INTEGER"/>
            <result property="doctorId" column="doctorId" jdbcType="BIGINT"/>
            <result property="date" column="date" jdbcType="VARCHAR"/>
            <result property="status" column="status" jdbcType="INTEGER"/>
            <result property="comment" column="comment" jdbcType="INTEGER"/>
            <result property="creationTime" column="creationTime" jdbcType="DATE"/>
    </resultMap>

</mapper>
